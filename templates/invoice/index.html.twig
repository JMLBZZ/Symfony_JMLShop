<!doctype html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>CAP NATION - Facture</title>
        <style>
            .text-center {
                text-align: center;
            }

            .text-right {
                text-align: right;
            }

            .text-left {
                text-align: left;
            }

            body {
                font-family: Roboto, sans-serif;
            }

            table {
                border-collapse: collapse;
            }
            table td, table th {
                padding:0.5rem;
                border-top: 1px solid #ccc;
            }
            .padtd{
                padding: 50px 0 !important;
            }
            .strongTotal{
                font-weight:bold;
            }
            h1, h2, h3, h4, h5 {
                padding-top:25px;
            }
            a{
                text-decoration:none;
                color:black;
            }
        </style>
    </head>
    <body>
        <div class="text-center">
            <img 
                style="width:120px;" 
                alt="Logo Cap Nation" 
                src="data:image/png;base64,"
            >
            <h3>Facture commande n°{{ order.id }}</h3>
            <table style="width: 100%;border-bottom:1px solid #ccc;">
                <tr >
                    <td style="width: 60%;" class="padtd">
                        <strong>CAP NATION</strong>
                        <br/>
                        8 rue Guénot,
                        <br/>
                        75011, Paris - France
                        <br/>
                        email@capnation.com
                        <br/>
                        <a href="tel:+33123456789">0123456789</a>
                    </td>
                    <td class="text-left" class="padtd">
                        {{ order.delivery|raw }}
                    </td>
                </tr>
            </table>
        </div>        

        <h5>Votre commande : {{ order.createdAt|date("d/m/Y")}}</h5>
        
        <table style="width: 100%;">
            <thead>
                <tr>
                    <th scope="col" class="text-left">Produit</th>
                    <th scope="col" class="text-left">Quantité</th>
                    <th scope="col" class="text-left">Prix unitaire H.T</th>
                    <th scope="col" class="text-left">TVA</th>
                    <th scope="col" class="text-right">Total T.T.C.</th>
                </tr>
            </thead>
            <tbody>
                {% for product in order.orderDetails %}
                    <tr>
                        <td>{{ product.productName }}</td>
                        <td>{{ product.productQuality }}</td>
                        <td>{{ product.productPrice|pricefilter }}</td>
                        <td>{{ product.productTva }}%</td>
                            {% set coeff = 1 + (product.productTva / 100) %}
                        <td class="text-right">{{ ((product.productPrice * coeff) * product.productQuality)|pricefilter }}</td>
                    </tr>
                {% endfor %}
                <tr>
                    <td>{{ order.carrierName }}</td>
                    <td>1</td>
                    <td></td>
                    <td></td>
                    <td class="text-right">{{ order.carrierPrice|pricefilter }}</td>
                </tr>
                <br/>
                <tfoot class="strongTotal">
                    <tr style="background-color:black; color:white;">
                        <td>Total H.T. :</td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td class="text-right">{{ (order.totalPriceTva - order.totalTva)|pricefilter }}</td>
                    </tr>
                    <tr style="background-color:grey; color:white;">
                        <td>Total TVA :</td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td class="text-right">{{ order.totalTva|pricefilter }}</td>
                        
                    </tr>
                    <tr style="background-color:red; color:white;">
                        <td>Total T.T.C. :</td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td class="text-right">{{ order.totalPriceTva|pricefilter }}</td>
                    </tr>
                </tfoot>
            </tbody>
        </table>

    </body>
</html>
